{
  "controller": "controller_xl.json",
  "mappings": [
    {
      "type": "device",
      "lom": "tracks.selected.device.selected",
      "range_maps": [
        {
          "|": "maps a range of midi encoders to a range of Ableton device parameters",
          "row": 2,
          "range": { "from": 1,  "to": 8 },
          "parameters": { "from": 1, "to": 8 }
        },
        {
          "row": 3,
          "range": { "from": 1, "to": 8 },
          "parameters": { "from": 9, "to": 16 }
        }
      ]
    },
    {
      "type": "mixer",
      "track": "selected",
      "mappings": {
        "volume": "r4-8",
        "pan": "r1-4",
        "mute": "r6-1",
        "solo": "r6-2",
        "sends": [
          "r1-5",
          "r1-6",
          "r1-7",
          "r1-8"
        ]
      }
    }
  ]
}