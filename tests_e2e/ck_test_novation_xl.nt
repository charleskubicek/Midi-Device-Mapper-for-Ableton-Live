controller: controller_xl.nt
ableton_dir: /Applications/Ableton Live 12 Suite.app
mappings:
    -
        type: device
        track: selected
        device: selected
        mappings:
            encoders:
                range: row-2:1-8,row-3:1-8
                parameters: 1-16
    -
        type: mixer
        track: selected
        mappings:
            volume: row-4:8
            pan: row-1:4
            mute: row-5:1
            solo: row-5:2
            sends: row-1:5-8
    -
        type: mixer
        track: master
        mappings:
            volume: row-4:7
    -
        type: track-nav
        mappings:
            left: row-4:2
            right: row-4:4
    -
        type: device-nav
        mappings:
#            left: row-4:2
#            right: row-4:4
            first: row-4:1
            last: row-4:6
         #   instrument: row-4:3
         #   CK_MIXER: row-4:5